            <a href="/view/{{this.id}}">{{hh_name}}</a>
            <p>Duration: {{this.hh_duration}}, Address: {{this.hh_address}}</p>
            <p>Drinks: {{this.hh_drinks}} Food: {{this.hh_food}} </p>
          </li>
          {{/each}}
        </ul>
        <p>pick your favorite :)</p>
      </div>
